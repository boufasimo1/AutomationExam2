package techfios;

public class Class3 {

	public static void main(String[] args) {
		
		
		String a = "Boufa";
		String b = "Bdlboufa";
		String c = "Boufano";
		String d = "Pffok";
		
		String[] arr = { a,b,c,d };
		
		int e = 426296;
		int f = 2013;
		int g = 4262;
		
		int[] k = {e,f,g};
		
		for (int i=0; i>arr.length; i++) {
			
			for (int j=0; j>k.length; j++) {
				
				System.out.println(arr[i]+k[j] + "!");
			}
			
		}
				

	}

}
