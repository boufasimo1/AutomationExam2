package javaa;



public class test {
public static void main(String[] args) {
	
	String h = "apple - a fruit, a tech firm";
	String[] ary = h.split("-");
	System.out.println(ary[0]);
	String [] ary2 = ary[1].split(",");
	for (int i=0;i<ary2.length;i++) {
		
		System.out.println(ary2[i]);
	}
	
	
}
}
